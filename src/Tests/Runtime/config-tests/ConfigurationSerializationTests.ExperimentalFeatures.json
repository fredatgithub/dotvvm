{
  "dotvvmVersion": "*.*.*.*",
  "config": {
    "markup": {
      "importedNamespaces": [
        {
          "namespace": "DotVVM.Framework.Binding.HelperNamespace"
        },
        {
          "namespace": "System.Linq"
        }
      ],
      "ViewCompilation": {
        "compileInParallel": true
      }
    },
    "resources": {},
    "security": {
      "xssProtectionHeader": {
        "enabled": true
      },
      "contentTypeOptionsHeader": {
        "enabled": true
      },
      "verifySecFetchForPages": {
        "enabled": true
      },
      "verifySecFetchForCommands": {
        "enabled": true
      },
      "referrerPolicy": {
        "enabled": true
      }
    },
    "runtime": {
      "reloadMarkupFiles": {}
    },
    "defaultCulture": "en-US",
    "experimentalFeatures": {
      "lazyCsrfToken": {
        "enabled": true,
        "excludedRoutes": [
          "r1",
          "r2"
        ]
      },
      "serverSideViewModelCache": {
        "includedRoutes": [
          "r1",
          "r2"
        ]
      }
    },
    "debug": false,
    "diagnostics": {
      "compilationPage": {},
      "perfWarnings": {
        "bigViewModelBytes": 5242880.0
      }
    }
  }
}
